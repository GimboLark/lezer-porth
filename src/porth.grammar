@top File { (Identifier | Number | String | Intrinsics | Keywords | Control | Paranteces)+ }

@skip { space }

@tokens {
  space { @whitespace+ }
  Identifier { $[A-Za-z_]+ }
  Number { $[0-9]+ }
  String { '"' !["]* '"' }
  Intrinsics { '+' | '-' | '*' | '<' | '>' | '<=' | '>=' | '!=' | '=' }
  Keywords { 'dup' | 'print' | 'drop' | 'over'| 'proc' | 'main' }
  Control { 'while' }
  Paranteces { 'in' | 'end'}
}

@external propSource highlighting from "./highlight.js"
